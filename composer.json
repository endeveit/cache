{
    "name": "endeveit/cache",
    "version": "0.2",
    "type": "library",
    "description": "Simple caching library with support for tags.",
    "keywords": [
        "cache",
        "tags",
        "redis",
        "predis",
        "mongo",
        "mongodb",
        "sqlite",
        "mysql",
        "memcache",
        "memcached"
    ],
    "homepage": "http://github.com/endeveit/cache",
    "license": "MIT",
    "authors": [
        {
            "name": "Nikita Vershinin",
            "email": "endeveit@gmail.com",
            "homepage": "http://endeveit.net"
        }
    ],
    "require": {
        "php": ">=5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "predis/predis": "0.8.x",
        "squizlabs/php_codesniffer": "1.*"
    },
    "suggest": {
        "ext-memcache": "Allow using Memcache driver",
        "ext-mongo": "Allow using Mongo driver",
        "ext-phpredis": "Allow using Redis driver",
        "ext-xcache": "Allow using XCache driver",
        "predis/predis": "Allow using Predis driver"
    },
    "autoload": {
        "psr-0": { "Endeveit\\Cache\\": "src/" }
    }
}
